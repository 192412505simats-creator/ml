import numpy as np
from sklearn.tree import DecisionTreeClassifier

X = [[5.5,32,3,13],
     [6.0,64,4,15],
     [4.7,16,2,10],
     [5.2,32,3,12],
     [6.1,128,4,16],
     [4.5,16,2,9]]
y = np.array([1,2,1,1,3,1])

model = DecisionTreeClassifier()
model.fit(X, y)

new_mobile = [[5.8,64,3,14]]
print("Predicted Price Range:", model.predict(new_mobile)[0])
